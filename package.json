    "cross-env": "^5.0.0",
    "css-loader": "^0.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "git-info": "github:afeiship/git-info",
    "glob": "^7.0.6",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-pug-plugin": "^0.2.0",
