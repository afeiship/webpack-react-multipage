{
  "name": "webpack-react-multipage",
  "version": "0.0.1",
  "description": "React multipage app based on webpack.",
  "homepage": "git@github.com:afeiship/webpack-react-multipage.git",
  "author": {
    "name": "afei",
    "email": "1290657123@qq.com"
  },
  "scripts": {
    "analyzer": "webpack --config ./build/webpack.dll-analyzer.babel.js",
    "clean": "rimraf dist",
    "dll-dev": "webpack --config ./build/webpack.config.dll-dev.babel.js",
    "dll-prd": "webpack --config ./build/webpack.config.dll-prd.babel.js",
    "predev": "npm run clean && npm run dll-dev",
    "prebuild": "npm run clean && npm run dll-prd",
    "postbuild": "rimraf dist/vendors",
    "build": "NODE_ENV=production webpack -p --env=prd",
    "dev": "NODE_ENV=development webpack-dev-server --env=dev",
    "start": "node express.js"
  },
  "license": "MIT",
